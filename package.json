{
  "name": "remotion-template",
  "version": "1.0.0",
  "description": "My Remotion video",
  "scripts": {
    "start": "remotion preview src/index.tsx",
    "build": "remotion render src/index.tsx Main out/video.mov",
    "build:recordings": "remotion render src/index.tsx Recordings out/recordings.mov --image-format=png --pixel-format=yuva444p10le --codec=prores --prores-profile=4444",
    "build:conclusion": "remotion render src/index.tsx Conclusion out/conclusion.mov --image-format=png --pixel-format=yuva444p10le --codec=prores --prores-profile=4444",
    "upgrade": "remotion upgrade",
    "server": "ts-node server.tsx"
  },
  "repository": {},
  "license": "UNLICENSED",
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@remotion/bundler": "^2.3.2",
    "@remotion/cli": "^2.3.2",
    "@remotion/renderer": "^2.3.2",
    "@types/express": "^4.17.9",
    "@types/react": "^17.0.0",
    "express": "^4.17.1",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remotion": "^2.3.2",
    "ts-node": "^9.1.1",
    "typescript": "4.4.2"
  }
}
